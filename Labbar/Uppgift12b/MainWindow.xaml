<Window x:Class="Uppgift12.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift12"
        mc:Ignorable="d"
        Title="Medelvärde" Height="208" Width="390">
    <Grid>
        <Label Content="Mata in tal" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Totala medelvärdet" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="userInput" HorizontalAlignment="Left" Margin="10,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="medelVärde" HorizontalAlignment="Left" Margin="10,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="okknapp" Content="Ok" HorizontalAlignment="Left" Margin="149,35,0,0" VerticalAlignment="Top" Width="65" Click="okknapp_Click"/>
        <ListBox x:Name="listlåda" HorizontalAlignment="Left" VerticalAlignment="Top" Width="124" Margin="230,35,0,0" Height="125" SelectionChanged="listlåda_SelectionChanged"/>
        <Button x:Name="nollställ" Content="Nollställ" HorizontalAlignment="Left" Margin="149,63,0,0" VerticalAlignment="Top" Width="65" Click="nollställ_Click"/>
    </Grid>
</Window>
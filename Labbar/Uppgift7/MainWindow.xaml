        <Button x:Name="beräkningsknapp" Content="Beräkna" HorizontalAlignment="Left" Margin="26,89,0,0" VerticalAlignment="Top" Width="77" Click="beräkningsknapp_Click"/>
        <TextBox x:Name="tal1ruta" HorizontalAlignment="Left" Margin="26,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tal2ruta" HorizontalAlignment="Left" Margin="166,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="heltaltext" Content="Heltal" HorizontalAlignment="Left" Margin="28,149,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="heltalbox" HorizontalAlignment="Left" Margin="28,180,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="28,217,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="restbox" HorizontalAlignment="Left" Margin="28,248,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>

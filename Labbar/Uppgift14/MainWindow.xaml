        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift14"
        mc:Ignorable="d"
        Title="Inmatningskontroll" Height="150" Width="280">
    <Grid>
        <Label Content="Ange födelseår" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="playersInput" HorizontalAlignment="Left" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="åldersKnapp" Content="Beräkna ålder" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Click="åldersKnapp_Click"/>

    </Grid>
</Window>
